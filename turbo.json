{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "inputs": ["src/**", "public/**", "package.json"],
      "env": [
        "VITE_SUPABASE_URL",
        "VITE_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY"
      ]
    },
    "mobile#build": {
      "dependsOn": ["^build"],
      "outputs": ["android/app/build/**", "ios/build/**", ".expo/**"],
      "inputs": ["src/**", "app.config.ts", "babel.config.js", "metro.config.js", "package.json"],
      "env": ["EXPO_PUBLIC_SUPABASE_URL", "EXPO_PUBLIC_SUPABASE_ANON_KEY"]
    },
    "mobile#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "mobile#test": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "mobile#test:e2e": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "mobile#lint": {
      "outputs": []
    },
    "mobile#type-check": {
      "outputs": []
    },
    "admin#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "admin#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": ["src/**", "app/**", "public/**", "next.config.js", "package.json"],
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY"
      ]
    },
    "admin#lint": {
      "outputs": []
    },
    "admin#type-check": {
      "outputs": []
    },
    "web#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": ["app/**", "public/**", "next.config.js", "package.json"],
      "env": ["NEXT_PUBLIC_SUPABASE_URL", "NEXT_PUBLIC_SUPABASE_ANON_KEY"]
    },
    "web#lint": {
      "outputs": []
    },
    "web#type-check": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "outputs": []
    },
    "test": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "format": {
      "outputs": [],
      "cache": false
    },
    "format:check": {
      "outputs": []
    }
  },
  "remoteCache": {
    "enabled": true
  }
}
