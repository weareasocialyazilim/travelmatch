{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "VITE_SUPABASE_URL",
        "VITE_SUPABASE_ANON_KEY"
      ]
    },
    "mobile#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "android/app/build/**",
        "ios/build/**",
        ".expo/**"
      ],
      "env": [
        "EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY"
      ]
    },
    "mobile#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "mobile#test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "**/__tests__/**"
      ]
    },
    "mobile#test:e2e": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "mobile#lint": {
      "outputs": []
    },
    "mobile#type-check": {
      "outputs": []
    },
    "admin#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "admin#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**"
      ],
      "env": [
        "VITE_SUPABASE_URL",
        "VITE_SUPABASE_ANON_KEY"
      ]
    },
    "admin#lint": {
      "outputs": []
    },
    "admin#type-check": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "**/__tests__/**"
      ]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "format": {
      "outputs": [],
      "cache": false
    },
    "format:check": {
      "outputs": []
    }
  },
  "remoteCache": {
    "enabled": true
  }
}
