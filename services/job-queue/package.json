{
  "name": "@lovendo/job-queue",
  "version": "1.0.0",
  "private": true,
  "description": "Background job queue service using BullMQ and Redis",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "worker": "tsx src/workers/index.ts",
    "queue:ui": "bull-board",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "lint": "eslint src",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@bull-board/api": "^5.10.0",
    "@bull-board/express": "^5.10.0",
    "@bull-board/ui": "^5.10.0",
    "@lovendo/shared": "workspace:*",
    "@supabase/supabase-js": "^2.88.0",
    "bullmq": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.30",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.7.0",
    "typescript": "~5.9.2"
  }
}
