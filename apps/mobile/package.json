{
  "name": "@travelmatch/mobile",
  "version": "1.0.0",
  "description": "TravelMatch Mobile App - React Native + Expo",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:staged": "jest --bail --findRelatedTests --passWithNoTests",
    "lint": "NODE_OPTIONS='--max-old-space-size=12288' eslint --cache --cache-location .eslintcache src --ext .ts,.tsx",
    "lint:fix": "NODE_OPTIONS='--max-old-space-size=12288' eslint --cache --cache-location .eslintcache src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "storybook": "sb-rn-get-stories && expo start",
    "storybook:generate": "sb-rn-get-stories",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run test:ci",
    "clean": "rm -rf node_modules .expo dist coverage",
    "prebuild": "expo prebuild",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "secrets:pull": "infisical export --env=dev --format=dotenv > .env",
    "secrets:pull:prod": "infisical export --env=prod --format=dotenv > .env.production",
    "dev:secure": "infisical run --env=dev -- pnpm dev",
    "build:secure": "infisical run --env=prod -- pnpm build:all",
    "db:types": "cd ../.. && supabase gen types typescript --linked > apps/mobile/src/types/database.generated.ts",
    "db:types:local": "cd ../.. && supabase gen types typescript --local > apps/mobile/src/types/database.generated.ts"
  },
  "dependencies": {
    "@expo/vector-icons": "~15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@infisical/sdk": "^4.0.6",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/stack": "^7.6.13",
    "@rnmapbox/maps": "^10.2.10",
    "@sentry/react-native": "~7.8.0",
    "@shopify/flash-list": "^2.2.0",
    "@supabase/supabase-js": "^2.88.0",
    "@travelmatch/design-system": "workspace:*",
    "@travelmatch/shared": "workspace:*",
    "axios": "^1.13.2",
    "expo": "~54.0.29",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-device": "~8.0.10",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-local-authentication": "^17.0.8",
    "expo-localization": "~17.0.8",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.15",
    "expo-screen-capture": "~8.0.9",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.15",
    "i18next": "^25.7.3",
    "pako": "^2.1.0",
    "posthog-react-native": "^4.16.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.68.0",
    "react-i18next": "^16.5.0",
    "react-native": "0.83.0",
    "react-native-device-info": "^15.0.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^4.1.0",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.2",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@eslint/js": "^9.39.2",
    "@storybook/react-native": "^10.1.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/pako": "^2.0.4",
    "@types/prop-types": "^15.7.15",
    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "lucide-react-native": "^0.561.0",
    "prettier": "^3.7.4",
    "react-test-renderer": "19.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "expo": {
    "install": {
      "exclude": [
        "@shopify/flash-list"
      ]
    }
  }
}
