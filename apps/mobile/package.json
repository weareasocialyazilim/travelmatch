{
  "name": "@travelmatch/mobile",
  "version": "1.0.0",
  "description": "TravelMatch Mobile App - React Native + Expo",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:staged": "jest --bail --findRelatedTests --passWithNoTests",
    "test:e2e": "maestro test ../../.maestro/",
    "test:e2e:login": "maestro test ../../.maestro/01-login.yaml",
    "test:e2e:signup": "maestro test ../../.maestro/07-signup.yaml",
    "test:e2e:cloud": "maestro cloud ../../.maestro/",
    "lint": "NODE_OPTIONS='--max-old-space-size=12288' eslint --cache --cache-location .eslintcache src --ext .ts,.tsx",
    "lint:fix": "NODE_OPTIONS='--max-old-space-size=12288' eslint --cache --cache-location .eslintcache src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "storybook": "sb-rn-get-stories && expo start",
    "storybook:generate": "sb-rn-get-stories",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run test:ci",
    "clean": "rm -rf node_modules .expo dist coverage",
    "prebuild": "expo prebuild",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "@sentry/react-native": "~7.2.0",
    "@supabase/supabase-js": "^2.86.2",
    "@travelmatch/design-system": "workspace:*",
    "@travelmatch/shared": "workspace:*",
    "axios": "^1.13.2",
    "expo": "~54.0.0",
    "expo-blur": "~13.0.2",
    "expo-constants": "~16.0.0",
    "expo-device": "~6.0.2",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.10",
    "expo-haptics": "~13.0.1",
    "expo-image-picker": "~15.0.7",
    "expo-linear-gradient": "~13.0.2",
    "expo-local-authentication": "^17.0.8",
    "expo-localization": "~15.0.3",
    "expo-location": "~17.0.1",
    "expo-notifications": "~0.30.0",
    "expo-screen-capture": "~6.0.1",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~2.0.0",
    "i18next": "^25.7.0",
    "pako": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.67.0",
    "react-i18next": "^16.3.5",
    "react-native": "0.82.1",
    "react-native-device-info": "^15.0.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "1.18.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.0",
    "react-native-worklets": "0.5.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-native-community/slider": "^5.1.1",
    "@sentry/tracing": "^7.120.4",
    "@storybook/react-native": "^10.1.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/pako": "^2.0.4",
    "@types/prop-types": "^15.7.15",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.14",
    "lucide-react-native": "^0.556.0",
    "prettier": "^2.8.8",
    "react-native-safe-area-context": "^5.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
