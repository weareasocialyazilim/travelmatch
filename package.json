{
  "name": "travelmatch-new",
  "version": "1.0.0",
  "private": true,
  "description": "TravelMatch Monorepo - Social travel platform",
  "scripts": {
    "dev": "turbo run dev",
    "dev:mobile": "pnpm --filter @travelmatch/mobile dev",
    "dev:web": "pnpm --filter @travelmatch/web dev",
    "dev:admin": "pnpm --filter @travelmatch/admin dev",
    "build": "turbo run build",
    "build:analyze": "ANALYZE=true turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "setup:env": "node scripts/setup-env.mjs",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push",
    "db:migrate:new": "supabase migration new",
    "db:generate-types": "./scripts/generate-db-types.sh remote",
    "db:generate-types:local": "./scripts/generate-db-types.sh local",
    "db:diff": "supabase db diff",
    "db:status": "supabase status",
    "db:test:rls": "supabase/tests/run_rls_tests.sh local",
    "db:test:rls:staging": "supabase/tests/run_rls_tests.sh staging",
    "db:test:storage": "supabase/tests/run_rls_tests.sh local storage",
    "db:test:realtime": "supabase/tests/run_rls_tests.sh local realtime",
    "db:test:functions": "supabase/tests/run_rls_tests.sh local functions",
    "db:test:mutations": "supabase/tests/run_rls_tests.sh local mutations",
    "seed:local": "tsx scripts/seed-database.ts",
    "seed:staging": "tsx scripts/seed-database.ts",
    "db:test:all": "supabase/tests/run_rls_tests.sh local all",
    "db:test:all:staging": "supabase/tests/run_rls_tests.sh staging all",
    "test:integration": "jest --config jest.config.js --testPathPattern=tests/integration",
    "test:integration:watch": "jest --config jest.config.js --testPathPattern=tests/integration --watch",
    "test:integration:payment": "jest --config jest.config.js tests/integration/paymentFlow",
    "test:integration:proof": "jest --config jest.config.js tests/integration/proofFlow",
    "test:integration:feed": "jest --config jest.config.js tests/integration/feedFlow",
    "test:coverage:full": "jest --coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=html",
    "test:coverage:check": "jest --coverage --coverageReporters=text-summary",
    "test:unit": "pnpm --filter @travelmatch/mobile test -- --testPathPattern='__tests__/(utils|hooks|lib)' --coverage",
    "test:components": "pnpm --filter @travelmatch/mobile test -- --testPathPattern='__tests__/components' --coverage",
    "test:screens": "pnpm --filter @travelmatch/mobile test -- --testPathPattern='__tests__/screens' --coverage",
    "test:e2e:flows": "pnpm --filter @travelmatch/mobile test -- --testPathPattern='__tests__/flows' --coverage",
    "test:visual": "turbo run chromatic",
    "test:visual:ci": "turbo run chromatic:ci",
    "test:all": "pnpm run test && pnpm run test:integration && pnpm run test:visual",
    "perf:lighthouse": "lhci autorun",
    "perf:check": "node scripts/performance-check.js",
    "prepare": "husky",
    "validate:hooks": "node scripts/validate-husky-setup.js",
    "tm": "node bin/tm.mjs"
  },
  "bin": {
    "tm": "./bin/tm.mjs"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@faker-js/faker": "^9.3.0",
    "@supabase/supabase-js": "^2.39.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "openai": "^6.10.0",
    "prettier": "^2.8.8",
    "stripe": "^20.0.0",
    "supabase": "^1.200.0",
    "tsx": "^4.7.0",
    "turbo": "^2.3.3",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
