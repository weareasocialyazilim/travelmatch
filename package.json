{
  "name": "travelmatch-new",
  "version": "1.0.0",
  "private": true,
  "description": "TravelMatch Monorepo - Social travel platform",
  "scripts": {
    "dev": "turbo run dev",
    "dev:mobile": "pnpm --filter @travelmatch/mobile dev",
    "dev:web": "pnpm --filter @travelmatch/web dev",
    "dev:admin": "pnpm --filter @travelmatch/admin dev",
    "dev:services": "docker-compose up ml-service job-queue -d",
    "dev:all": "docker-compose up web admin -d && pnpm dev",
    "build": "turbo run build",
    "build:mobile": "pnpm --filter @travelmatch/mobile build",
    "build:web": "pnpm --filter @travelmatch/web build",
    "build:admin": "pnpm --filter @travelmatch/admin build",
    "build:analyze": "ANALYZE=true turbo run build",
    "docker:build": "docker-compose build web admin",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "setup:env": "node scripts/setup-env.mjs",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push",
    "db:migrate:new": "supabase migration new",
    "db:generate-types": "./scripts/generate-db-types.sh remote",
    "db:generate-types:local": "./scripts/generate-db-types.sh local",
    "db:diff": "supabase db diff",
    "db:status": "supabase status",
    "db:test:rls": "supabase/tests/run_rls_tests.sh local",
    "db:test:rls:staging": "supabase/tests/run_rls_tests.sh staging",
    "db:test:storage": "supabase/tests/run_rls_tests.sh local storage",
    "db:test:realtime": "supabase/tests/run_rls_tests.sh local realtime",
    "db:test:functions": "supabase/tests/run_rls_tests.sh local functions",
    "db:test:mutations": "supabase/tests/run_rls_tests.sh local mutations",
    "seed:local": "tsx scripts/seed-database.ts",
    "seed:staging": "tsx scripts/seed-database.ts",
    "db:test:all": "supabase/tests/run_rls_tests.sh local all",
    "db:test:all:staging": "supabase/tests/run_rls_tests.sh staging all",
    "test:visual": "turbo run chromatic",
    "test:visual:ci": "turbo run chromatic:ci",
    "prepare": "husky",
    "lint-staged": "NODE_OPTIONS='--max-old-space-size=4096' npx lint-staged --concurrent false --no-stash --debug",
    "validate:hooks": "node scripts/validate-husky-setup.js",
    "tm": "node bin/tm.mjs",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "bin": {
    "tm": "./bin/tm.mjs"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@babel/preset-env": "^7.24.0",
    "@codecov/webpack-plugin": "^1.9.1",
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.3.0",
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "expo-module-scripts": "^5.0.8",
    "husky": "^9.1.7",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.7",
    "openai": "^6.10.0",
    "prettier": "^3.7.4",
    "stripe": "^20.0.0",
    "supabase": "^2.67.3",
    "tsx": "^4.7.0",
    "turbo": "^2.3.3",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.19.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "expo": "~54.0.29",
    "metro": "0.80.12",
    "metro-cache": "0.80.12",
    "metro-core": "0.80.12",
    "metro-runtime": "0.80.12",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-native": "0.81.5",
      "fast-xml-parser": "^5.3.3"
    }
  }
}
