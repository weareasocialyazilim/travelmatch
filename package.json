{
  "name": "lovendo",
  "version": "1.0.0",
  "private": true,
  "description": "Lovendo Monorepo - Social experience platform",
  "scripts": {
    "dev": "turbo run dev",
    "dev:mobile": "pnpm --filter @lovendo/mobile dev",
    "dev:web": "pnpm --filter @lovendo/web dev",
    "dev:admin": "pnpm --filter @lovendo/admin dev",
    "dev:services": "docker-compose up ml-service job-queue -d",
    "dev:all": "docker-compose up web admin -d && pnpm dev",
    "build": "turbo run build",
    "build:mobile": "pnpm --filter @lovendo/mobile build",
    "build:web": "pnpm --filter @lovendo/web build",
    "build:admin": "pnpm --filter @lovendo/admin build",
    "build:analyze": "ANALYZE=true turbo run build",
    "docker:build": "docker-compose build web admin",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "turbo run test",
    "test:e2e": "playwright test",
    "test:e2e:web": "playwright test --project=web:chromium",
    "test:e2e:admin": "playwright test --project=admin:chromium",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "setup:env": "node scripts/setup-env.mjs",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:reset": "npx supabase db reset",
    "db:migrate": "npx supabase db push",
    "db:migrate:new": "npx supabase migration new",
    "db:generate-types": "./scripts/generate-db-types.sh remote",
    "db:generate-types:local": "./scripts/generate-db-types.sh local",
    "db:diff": "npx supabase db diff",
    "db:status": "npx supabase status",
    "db:test:rls": "supabase/tests/run_rls_tests.sh local",
    "db:test:rls:staging": "supabase/tests/run_rls_tests.sh staging",
    "db:test:storage": "supabase/tests/run_rls_tests.sh local storage",
    "db:test:realtime": "supabase/tests/run_rls_tests.sh local realtime",
    "db:test:functions": "supabase/tests/run_rls_tests.sh local functions",
    "db:test:mutations": "supabase/tests/run_rls_tests.sh local mutations",
    "seed:local": "tsx scripts/seed-database.ts",
    "seed:staging": "tsx scripts/seed-database.ts",
    "db:test:all": "supabase/tests/run_rls_tests.sh local all",
    "db:test:all:staging": "supabase/tests/run_rls_tests.sh staging all",
    "db:smoke": "./scripts/db-smoke.sh",
    "test:visual": "turbo run chromatic",
    "test:visual:ci": "turbo run chromatic:ci",
    "qa:pr": "pnpm lint && pnpm type-check && pnpm format:check && pnpm test -- --ci --maxWorkers=2 && pnpm test:e2e:web && pnpm test:e2e:admin && pnpm db:smoke",
    "qa:rc": "pnpm build && pnpm test:e2e && pnpm db:test:all && pnpm test:visual:ci",
    "prepare": "husky",
    "validate:hooks": "node scripts/validate-husky-setup.js",
    "android": "pnpm --filter @lovendo/mobile android",
    "ios": "pnpm --filter @lovendo/mobile ios",
    "android:prod": "pnpm --filter @lovendo/mobile android:prod",
    "ios:prod": "pnpm --filter @lovendo/mobile ios:prod",
    "ios:simulator": "pnpm --filter @lovendo/mobile ios",
    "ios:device": "pnpm --filter @lovendo/mobile ios -- --device",
    "ios:pods": "cd apps/mobile/ios && pod install",
    "ios:pods:update": "cd apps/mobile/ios && rm -rf Pods Podfile.lock && pod install --repo-update",
    "ios:clean": "cd apps/mobile/ios && rm -rf Pods Podfile.lock build && pod install",
    "mobile:prebuild": "pnpm --filter @lovendo/mobile prebuild",
    "mobile:start": "pnpm --filter @lovendo/mobile start"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@babel/preset-env": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.49.1",
    "@supabase/supabase-js": "^2.88.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chromatic": "^13.3.5",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "expo-module-scripts": "^5.0.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "openai": "^6.14.0",
    "prettier": "^3.7.4",
    "react-test-renderer": "19.1.0",
    "tsx": "^4.21.0",
    "turbo": "^2.6.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "engines": {
    "node": "22.22.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.28.0",
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-test-renderer": "19.1.0",
      "react-native": "0.81.5",
      "react-native-safe-area-context": "5.6.2",
      "fast-xml-parser": "^4.5.0",
      "glob": "^9.0.0",
      "qs": ">=6.14.1",
      "zod": "^3.24.1",
      "react-native-qrcode-svg>react": "19.1.0",
      "react-native-qrcode-svg>react-native": "0.81.5",
      "react-native-svg>react": "19.1.0",
      "react-native-svg>react-native": "0.81.5",
      "@react-native/virtualized-lists>react": "19.1.0",
      "@react-native/virtualized-lists>react-native": "0.81.5",
      "@react-native-async-storage/async-storage>react-native": "0.81.5",
      "react-native-reanimated": "~4.1.6"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-native"
      ],
      "allowAny": [
        "react",
        "react-native"
      ]
    },
    "onlyBuiltDependencies": [
      "@datadog/mobile-react-native",
      "@sentry/cli",
      "@swc/core",
      "core-js",
      "esbuild",
      "msgpackr-extract",
      "protobufjs",
      "sharp",
      "unrs-resolver"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "expo": "~54.0.31",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-svg": "^15.15.1"
  }
}
